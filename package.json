{
  "name": "time-track-api",
  "version": "1.0.0",
  "description": "Express server with PostgreSQL DB based API for staff Work Hour and PTO Management",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon server.js",
    "test": "mocha --require test/setup.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "author": "Rodrigo Hervas",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "knex": "^0.20.13",
    "minimist": "^1.2.5",
    "morgan": "^1.9.1",
    "pg": "^8.0.0",
    "postgrator-cli": "^3.3.0",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": []
}
